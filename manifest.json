{
  "manifest_version" : 2,
  "name": "ChangeNotifier",
  "version": "0.1",
  "description": "Alerts users for changes in webpages.",
  "icons": { "256": "assets/icon_256.png" },
  "permissions": ["<all_urls>", "activeTab", "tabs"],
  "background": {
    "scripts": ["libraries/jquery.js", "assets/js/background.js"]
   },
  "browser_action": {
    "default_title": "",
    "default_icon" : "assets/icon_256.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}